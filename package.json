{
  "name": "starmap",
  "version": "1.0.0",
  "description": "星図表示アプリケーション",
  "main": "src/frontend/main.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "start": "electron .",
    "dev:electron": "set NODE_ENV=development && electron .",
    "dev:backend": "cd src/backend && python -m uvicorn main:app --reload --port 8000",
    "dev:frontend": "webpack serve --mode development --port 3002",
    "dev:mobile": "webpack serve --mode development --port 3003 --env platform=mobile",
    "build": "webpack --mode production",
    "build:mobile": "webpack --mode production --env platform=mobile",
    "test": "jest",
    "test:visual": "jest src/utils/test/starmap.test.js",
    "test:visual:dev": "concurrently \"npm run dev:frontend\" \"wait-on http://localhost:3002 && npm run test:visual\""
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@date-io/dayjs": "^3.2.0",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.8",
    "@mui/icons-material": "^5.15.7",
    "@mui/material": "^5.15.7",
    "@mui/x-date-pickers": "^7.25.0",
    "@react-three/drei": "^9.92.7",
    "@react-three/fiber": "^8.15.12",
    "@reduxjs/toolkit": "^2.5.1",
    "axios": "^1.6.2",
    "dayjs": "^1.11.13",
    "electron": "^28.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.2.0",
    "three": "^0.160.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "css-loader": "^6.10.0",
    "electron-builder": "^24.9.1",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "puppeteer": "^24.2.1",
    "style-loader": "^3.3.4",
    "wait-on": "^8.0.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "jest": {
    "testEnvironment": "node",
    "testTimeout": 30000,
    "setupFilesAfterEnv": [
      "<rootDir>/src/utils/test/setup.js"
    ]
  }
}
