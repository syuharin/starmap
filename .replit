# Replit configuration file

# Specifies the command to run when the "Run" button is clicked.
run = "cd src/backend && uvicorn main:app --host 0.0.0.0 --port 8080"

# Specifies the programming language.
language = "python3"
modules = ["python-3.11"]

[nix]

[[ports]]
localPort = 8080
externalPort = 80

# Optional: Specify the entry point if needed for other Replit features.
# entrypoint = "src/backend/main.py"
